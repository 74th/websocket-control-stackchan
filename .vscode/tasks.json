{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "run voicevox",
      "command": "docker",
      "type": "shell",
      "args": [
        "compose", "run", "--rm", "--service-ports", "voicevox"
      ],
      "problemMatcher": [
        "$tsc"
      ],
      "presentation": {
        "reveal": "silent"
      },
      "options": {
        "cwd": "${workspaceFolder}/server"
      },
      "group": "none"
    }
  ]
}
